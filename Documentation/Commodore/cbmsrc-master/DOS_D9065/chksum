.PAGE 'CHECKSUM'
	*=$005E
TEMP	*=*+2
	*=$0500
	CLD
	SEI
	LDA #0
	STA $1000
	CMP $1000
CHKPRO
	BNE CHKPRO
	LDA #$10
	JSR CHKSUM
	STA $1000
	BRK
	LDA #0
	STA $3000
	CMP $3000
	BNE CHKPRO
	LDA #$30
	JSR CHKSUM
	STA $3000
	BRK
CHKSUM
	CLC
	STA TEMP+1
	LDA #0
	STA TEMP
	LDX #32
	LDY #0
CHK1
	ADC (TEMP)Y
	INY
	BNE CHK1
	INC TEMP+1
	DEX
	BNE CHK1
	ADC #0
	STA TEMP+1
	LDA #$C0
	SEC
	SBC TEMP+1
	SBC #0
	RTS
.END
