.PAGE 'DUPLICATE'
; DUPLICATE DISK
.SKIP
DUPLCT
	LDA #BADCMD
	JMP CMDERR
.SKIP
; TRANSFER FORMAT CODE TO BUFFER 0
;  & START CONTROLLER FORMATTING
FORMAT
	LDA #$4C
	STA BUFS+$300
	LDA #<FORMT
	STA BUFS+$301
	LDA #>FORMT
	STA BUFS+$302
;
	LDA #3
	JSR SETH
	LDA DRVNUM
	ORA #EXEC
	STA JOBS+3
FMT105	LDA JOBS+3
	BMI FMT105
	CMP #2
	BCC FMT110
	LDA #3
	LDX #0
	JMP ERROR
FMT110	RTS
;
;
.END
