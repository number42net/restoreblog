.PAGE 'EDT5SRC'
;
	*=BEGIN+$0721
;
RUNTB	.BYT 'D',$CC,'"*',$D,'RUN',$D
.SKI 2
; ****** 6845 CRTC TEXT MODE ******
.SKI 2
TEXT	.BYT $31,$28,$29,$0F
	.BYT $27,$00,$19,$20
	.BYT $00,$09,$00,$00
	.BYT $10,$00,$00,$00
	.BYT $00,$00
.SKI 2
; ****** 6845 CRTC GRAPHIC MODE ******
.SKI 2
GRAF	.BYT $31,$28,$29,$0F
	.BYT $31,$00,$19,$25
	.BYT $00,$07,$00,$00
	.BYT $10,$00,$00,$00
	.BYT $00,$00
.SKI 3
.SKI 2
; ****** TONE TABLE FOR BELL ******
.SKI 2
TONES	.BYT 14,30,62,126
	.BYT 62,30,14
.SKI 3
;****** ADDRESS OF SCREEN LINES ******
;
LINZ0	= SCNRAM
