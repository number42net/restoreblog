.PAG   'INDIRECTION LDA/STA KLUDGES'
YLARMO	;*** ARGMO ***
	SEI
	NOP
	LDA (ARGMO)Y
	CLI
	RTS
;
YSARMO	
	SEI
	NOP
	STA (ARGMO)Y
	CLI
	RTS
;
