;----- SPRITE MOTION DISTANCE TABLE

WALKY
	.BYTE $FE,$FE,$FE,$FF,$00,$01,$02,$02
	.BYTE $02,$02,$02,$01,$00,$FF,$FE,$FE
WALKX
	.BYTE $00,$01,$02,$02,$02,$02,$02,$01
	.BYTE $00,$FF,$FE,$FE,$FE,$FE,$FE,$FF

