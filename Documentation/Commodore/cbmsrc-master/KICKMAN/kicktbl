	.BYTE %01000000
	.BYTE %10000000

MASKZ
	.BYTE %11111110
	.BYTE %11111101
	.BYTE %11111011
	.BYTE %11110111
	.BYTE %11101111
	.BYTE %11011111
	.BYTE %10111111
	.BYTE %01111111

