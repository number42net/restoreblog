.PAGE 'CHKSUM
	*= $BF00
	SEI
	LDX #$FF
	TXS
	CLD
	LDA #0
	STA CCHKSM
	CMP CCHKSM
CHKPRO
	BNE CHKPRO
	LDA #$C0
	JSR GENCHK
	STA CCHKSM
	LDA #0
	STA ECHKSM
	CMP ECHKSM
	BNE CHKPRO
	LDA #$E0
	JSR GENCHK
	STA ECHKSM
	JMP *
;
GENCHK	;.A=ADDRESS
	CLC
	PHA
	STA TEMP+1
	LDA #0
	STA TEMP
	LDX #32
	LDY #0
CHK1
	ADC (TEMP)Y
	INY
	BNE CHK1
;
	INC TEMP+1
	DEX
	BNE CHK1
;
	ADC #0
	STA TEMP+1
	PLA
	SEC
	SBC TEMP+1
	SBC #0          ;.A=CHECKSUM BYTE
	RTS
.END
