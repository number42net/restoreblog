.PAGE 'ROMTBL'
;
; JUMP TABLE TO ROUTINES
;
	*=$FF00
GETBUF
	JMP JGETBF
FREBUF
	JMP JFREBF
FRETS
	JMP JFRETS
USEDTS
	JMP JUSED
NXTTS
	JMP JNXTTS
INTTS
	JMP JINTTS
MAXSEC
	JMP JMXSEC
ERROR
	JMP JERROR
AUTOI
	JMP JAUTOI
SETH
	JMP JSETH
ERRMSG
	JMP JERRMS
SETLDS
	JMP JSETLD
ILLTS
	JMP JILLTS
IDLE
	JMP (VECIDL)
SCRBAM
	JMP JSCRBM
CMDERR
	JMP JCMDER
CMDER2
	JMP JCDER2
TSTJOB
	JMP JTSTJB
ATNIRQ
	JMP (VECIEE)
DOJOB
	JMP JDOJOB
NXT0
	JMP JNXT0
FORMAT
	JMP JFORMT
.PAGE
	*=$FF50
;
; COMMAND SEARCH TABLE
;
CMDTBL
	.BYT 'VIDMBUP&CRSN'
NCMDS	=*-CMDTBL
;
; JUMP TABLE LOW
;
CJUMPL
	.BYT <VERDIR
	.BYT <INTDRV
	.BYT <DUPLCT
	.BYT <MEM
	.BYT <BLOCK
	.BYT <USER
	.BYT <RECORD
	.BYT <UTLODR
	.BYT <DSKCPY
	.BYT <RENAME
	.BYT <SCRTCH
	.BYT <NEW
	*=CJUMPL+NCMDS
;
; JUMP TABLE HIGH
;
CJUMPH
	.BYT >VERDIR
	.BYT >INTDRV
	.BYT >DUPLCT
	.BYT >MEM
	.BYT >BLOCK
	.BYT >USER
	.BYT >RECORD
	.BYT >UTLODR
	.BYT >DSKCPY
	.BYT >RENAME
	.BYT >SCRTCH
	.BYT >NEW
	*=CJUMPH+NCMDS
.SKIP
VAL=0 ; VALIDATE (VERIFY) CMD #
.SKIP
;
; STRUCTURE IMAGES FOR CMDS
;
PCMD	= 9             ; # OF PARSED COMMANDS
	.BYT %01010001  ; DISKCOPY
STRUCT	=*-PCMD         ; CMDS NOT PARSED
	.BYT %11011101  ; RENAME
	.BYT %00011100  ; SCRATCH
	.BYT %10011110  ; NEW
LDCMD	=*-STRUCT       ; LOAD CMD IMAGE
	.BYT %00011100  ; LOAD
;            --- ---
;            PGDRPGDR
;            FS1 FS2
.SKIP
;   BIT REPS:  NOT PATTERN
;              NOT GREATER THAN ONE FILE 
;              NOT DEFAULT DRIVE(S) 
;              REQUIRED FILENAME
.SKIP
MODLST	.BYT 'RWAM'     ; MODE TABLE
NMODES	=*-MODLST
;
; FILE TYPE TABLE
;
TPLST	.BYT 'DSPUL'
TYPLST	.BYT 'DSPUR'    ; DEL, SEQ, PRG, USR, REL
NTYPES	=*-TYPLST
TP1LST	.BYT 'EERSE'
TP2LST	.BYT 'LQGRL'
;
; ERROR FLAG VARS FOR BIT
;
ER00	.BYT 0
ER0	.BYT $3F
ER1	.BYT $7F
ER2	.BYT $BF
ER3	.BYT $FF
;
IPBM	.BYTE $41,$42
.END
