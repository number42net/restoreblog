SYSTEM	= 0             ;CBM SYSTEM =0  PET SYSTEM =1
SYSAGE	= 1             ;NONSPLIT = 0   SPLIT-BUS = 1
SYSSIZ	= 1             ;NO PACKING = 0  PACKING = 1
.LIB DISCLAIMER
.LIB DECLARE
.LIB EQUATE
	* = KERNAL
	JMP XXXZZZ      ;COLD START VECTOR
	NOP
	* = KERNAL+$0E00 ;3.50K CODE SPACE (SCREEN EDITOR)
S0
TIMB
XXXZZZ	CLD
	JSR CLRCH
XXXZZ1	JSR BASIN
	JMP XXXZZ1
.LIB MESSAGES
.LIB IEEE
.LIB RS232
.LIB CHANNELIO
.LIB OPENCHANNEL
.LIB CLOSE
.LIB CLALL
.LIB OPEN
.LIB LOAD
.LIB SAVE
.LIB TIME
