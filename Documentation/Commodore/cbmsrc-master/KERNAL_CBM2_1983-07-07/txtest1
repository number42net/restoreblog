.PAG 'TXTEST1'
; 02/28/83 BP
; TRANSFER EXEC ROUTINE TEST FOR EXAMPLE 1
;
;SAMPLE PROGRAM PRINTS TEST MESSAGE UNTIL STOP KEY IS DOWN
;
	*=$078B         ;THIS MUST MATCH CALL FROM SEGMENT $F
START	LDY #$00
START2	LDA MSG,Y       ;GET CHAR FROM STRING
	JSR BSOUT       ;AND PRINT TO SCREEN
	INY             ;NEXT CHA
	CPY #MSGEND-MSG ;DONE
	BNE START2      ;NOPE
;
	JSR STOP        ;YES. CHECK FOR STOP KEY DOWN
	BNE START       ;NO GO DO AGAIN
	BRK             ;STOP WITH A BRK
	NOP
	JMP START       ;IF CONTINUE THEN START ALL OVER AGAIN
MSG	.BYT 'TRANSFER TEST '
MSGEND
.FIL TXRAM
.END
